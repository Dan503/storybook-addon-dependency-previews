{
	"name": "storybook-addon-dependency-previews",
	"version": "0.1.0",
	"private": false,
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": "./dist/index.js",
		"./blocks": "./dist/blocks/index.js",
		"./panels": "./dist/panels/index.js",
		"./addon": "./dist/addon/index.js"
	},
	"bin": {
		"sb-deps": "./cli/sb-deps.cjs"
	},
	"typesVersions": {
		"*": {
			"blocks": [
				"dist/blocks/index.d.ts"
			]
		}
	},
	"files": [
		"dist",
		"cli",
		"depcruise.config.cjs"
	],
	"scripts": {
		"copy-css": "cpx \"src/**/*.module.css\" dist",
		"build": "rm -rf dist && tsc -p tsconfig.json && npm run copy-css",
		"dev:ts": "tsc -w -p tsconfig.json",
		"dev:css": "cpx \"src/**/*.module.css\" dist --watch",
		"dev": "concurrently -k -n TS,CSS \"pnpm dev:ts\" \"pnpm dev:css\""
	},
	"peerDependencies": {
		"react": ">=18",
		"react-dom": ">=18",
		"storybook": "^9.0.0"
	},
	"devDependencies": {
		"@storybook/blocks": "9.0.0-alpha.17",
		"@storybook/csf": "^0.1.13",
		"@storybook/types": "9.0.0-alpha.1",
		"@types/node": "^24.5.2",
		"@types/react": "^19.1.13",
		"@types/react-dom": "^19.1.9",
		"concurrently": "^9.2.1",
		"cpx2": "^8.0.0",
		"typescript": "^5.6.3",
		"vite": "^7.1.7"
	},
	"dependencies": {
		"@storybook/addon-links": "^9.1.8",
		"@storybook/manager-api": "9.0.0-alpha.1",
		"@storybook/preview-api": "9.0.0-alpha.1",
		"dependency-cruiser": "^16.6.0"
	},
	"storybook-addon": {
		"preset": "dist/addon/preset.js"
	}
}
