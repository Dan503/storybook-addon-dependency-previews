{
	"name": "storybook-addon-dependency-previews",
	"version": "0.1.0",
	"private": false,
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": "./dist/index.js",
		"./blocks": "./dist/blocks/index.js",
		"./panels": "./dist/panels/index.js",
		"./addon": "./dist/addon/index.js"
	},
	"bin": {
		"sb-deps": "./cli/sb-deps.cjs"
	},
	"typesVersions": {
		"*": {
			"blocks": [
				"dist/blocks/index.d.ts"
			]
		}
	},
	"files": [
		"dist",
		"cli",
		"depcruise.config.cjs"
	],
	"scripts": {
		"build": "rm -rf dist && tsc -p tsconfig.json",
		"dev": "tsc -w -p tsconfig.json"
	},
	"peerDependencies": {
		"react": ">=18",
		"react-dom": ">=18",
		"storybook": "^9.0.0"
	},
	"devDependencies": {
		"@storybook/blocks": "9.0.0-alpha.17",
		"@storybook/types": "9.0.0-alpha.1",
		"@types/node": "^24.5.2",
		"@types/react": "^19.1.13",
		"@types/react-dom": "^19.1.9",
		"typescript": "^5.6.3"
	},
	"dependencies": {
		"dependency-cruiser": "^16.6.0"
	}
}
