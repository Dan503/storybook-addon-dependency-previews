{
	"name": "storybook-addon-dependency-previews",
	"version": "0.1.0",
	"private": false,
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs",
			"types": "./dist/index.d.mts"
		},
		"./addon": {
			"import": "./dist/addon/index.mjs",
			"require": "./dist/addon/index.cjs",
			"types": "./dist/addon/index.d.mts"
		},
		"./cli/sb-deps": {
			"import": "./dist/cli/sb-deps.mjs",
			"require": "./dist/cli/sb-deps.cjs",
			"types": "./dist/cli/sb-deps.d.mts"
		}
	},
	"bin": {
		"sb-deps": "./dist/cli/sb-deps.cjs"
	},
	"typesVersions": {
		"*": {
			"blocks": [
				"dist/blocks/index.d.ts"
			]
		}
	},
	"sideEffects": [
		"**/*.css"
	],
	"files": [
		"dist/**"
	],
	"scripts": {
		"copy-css": "cpx \"src/**/*.module.css\" dist",
		"build": "rm -rf dist && tsdown && npm run copy-css && npm run typecheck",
		"dev": "npm-watch",
		"typecheck": "cross-env FORCE_COLOR=1 npm_config_color=always tsc -p tsconfig.json --noEmit --pretty true && tsc -p ./cli/tsconfig.json --noEmit --pretty true"
	},
	"watch": {
		"build": {
			"patterns": ["src","cli"],
			"extensions": "ts,tsx,css",
			"delay": 200
		}
	},
	"peerDependencies": {
		"react": ">=18",
		"react-dom": ">=18",
		"storybook": "^9.0.0"
	},
	"devDependencies": {
		"@storybook/blocks": "9.0.0-alpha.17",
		"@storybook/csf": "^0.1.13",
		"@storybook/types": "9.0.0-alpha.1",
		"@types/micromatch": "^4.0.9",
		"@types/node": "^24.7.2",
		"@types/react": "^19.1.13",
		"@types/react-dom": "^19.1.9",
		"concurrently": "^9.2.1",
		"cpx2": "^8.0.0",
		"cross-env": "^10.1.0",
		"npm-watch": "^0.13.0",
		"ts-dedent": "^2.2.0",
		"tsdown": "^0.15.7",
		"typescript": "^5.6.3",
		"vite": "^7.1.7"
	},
	"dependencies": {
		"@parcel/watcher": "^2.5.1",
		"@storybook/addon-links": "^9.1.8",
		"@storybook/manager-api": "9.0.0-alpha.1",
		"@storybook/preview-api": "9.0.0-alpha.1",
		"dependency-cruiser": "^16.6.0",
		"micromatch": "^4.0.8"
	},
	"storybook-addon": {
		"preset": "dist/addon/preset.cjs"
	}
}
