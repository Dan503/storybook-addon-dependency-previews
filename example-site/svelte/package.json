{
	"name": "example-site-svelte",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"sb": "sb-deps --watch --run-storybook",
		"sb:build": "pnpm --filter storybook-addon-dependency-previews build && node ../storybook-addon-package/dist/cli/sb-deps.cjs && storybook build",
		"sb:deps": "sb-deps"
	},
	"dependencies": {
		"@tanstack/svelte-form": "^1.28.0",
		"example-site-shared": "workspace:*",
		"svelte-headings": "^0.1.3"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^5.0.0",
		"@eslint/compat": "^2.0.1",
		"@eslint/js": "^9.39.2",
		"@storybook/addon-a11y": "^10.2.4",
		"@storybook/addon-docs": "^10.2.4",
		"@storybook/addon-svelte-csf": "^5.0.10",
		"@storybook/addon-vitest": "^10.2.4",
		"@storybook/svelte-vite": "^10.2.4",
		"@storybook/sveltekit": "^10.2.4",
		"@sveltejs/adapter-netlify": "^5.2.4",
		"@sveltejs/kit": "^2.50.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/forms": "^0.5.11",
		"@tailwindcss/vite": "^4.1.18",
		"@types/node": "^22",
		"@vitest/browser-playwright": "^4.0.18",
		"@vitest/coverage-v8": "^4.0.18",
		"dependency-cruiser": "17.3.8-beta-1",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-storybook": "^10.2.4",
		"eslint-plugin-svelte": "^3.14.0",
		"globals": "^17.1.0",
		"playwright": "^1.58.1",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"storybook": "^10.2.4",
		"storybook-addon-dependency-previews": "workspace:*",
		"svelte": "^5.48.2",
		"svelte-check": "^4.3.5",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.53.1",
		"vite": "^7.3.1",
		"vitest": "^4.0.18"
	}
}
